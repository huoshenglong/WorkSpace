<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.springmvc.dao.CashDAO">
        
          <select id="findCashRate" parameterType="String" resultType="Double">
        select cash_rate from cash_rate_table where start_cash=#{startcash} and end_cash=#{endcash}
          
         </select> 
  
</mapper>
